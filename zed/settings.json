{
  "agent": {
    "model_parameters": [],
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-3-7-sonnet-latest"
    },
    "version": "2"
  },

  "theme": "Catppuccin Latte",
  "ui_font_size": 14,
  "buffer_font_size": 12,
  "cursor_shape": "underline",
  "relative_line_numbers": true,
  "icon_theme": "Catppuccin Latte",

  "vim_mode": true,

  "lsp": {
    "pyright": {
      "settings": {
        "python": {
          "venvPath": "$HOME/.pyenv/"
        }
      }
    },
    "tinymist": {
      "settings": {
        "exportPdf": "never"
      }
    }
  },
  "autosave": "on_focus_change"
}
